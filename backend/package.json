{
  "name": "apiwave-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts --fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "db:migrate": "tsx src/scripts/migrate.ts",
    "db:seed": "tsx src/scripts/seed.ts"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "axios": "^1.4.0",
    "dotenv": "^16.3.0",
    "express-rate-limit": "^6.8.0",
    "multer": "^1.4.0",
    "uuid": "^9.0.0",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/pg": "^8.10.0",
    "@types/multer": "^1.4.0",
    "@types/uuid": "^9.0.0",
    "@types/node": "^20.4.0",
    "tsx": "^3.12.0",
    "vitest": "^0.34.0",
    "supertest": "^6.3.0",
    "@types/supertest": "^2.0.0"
  }
}